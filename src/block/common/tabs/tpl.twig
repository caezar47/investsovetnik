{% if view == "base" %}
<div class="{{prefix}}panel  {{group}}  ">			
	<ul class="{{prefix}}nav  {{mod}}">
		{% for value in list %}
		<li class="{{prefix}}item  {{mod}}  {{value.status}}">
			<a class="{{prefix}}link  {{mod}}" href="#{{value.id}}" data-toggle="tab">{{value.name}}</a>
		</li>
		{% endfor %}
	</ul>
</div>
{% for value in list %}
<div class="{{prefix}}pane  {{value.status}}  {{group}}" id="{{value.id}}">
	{% if value.name %}
		<div class="text__block">{{value.note}}</div>

	{% endif %}
	{# {% if value.type == "events" %}
		{% include "../modals/tpl.twig" with {
			prefix:			"modal__", 
			mod:			"is--inline", 
			ID: 			"modal-events", 
			heading_level: 	'h2',
			heading_mod: 	'is--h1  is--modal  visible--md',
			heading: 		'Лента событий',
			list: 			value.list,
		} %}
	{% endif %}
	{% if value.type == "chat" %}
		{% include "../modals/tpl.twig" with {
			prefix: 		"modal__", 
			mod:			"is--inline  is--chat", 
			ID: 			"modal-chat",	
			heading: 		'Чат с менеджером',
			heading_level: 	'h2',
			heading_mod: 	'is--h1  is--modal  visible--md',
			chat: 			chat,
		} %} 	
	{% endif %}
	{% if value.type == "table" %}
		{% include "../table/tpl.twig" with {
			prefix:"table",
			mod: "",
			head:table_head,
			body:table_body,
		} %}
	{% endif %}
	{% if value.type == "form" %}
		{% include "../form/form.twig" with {
			view: 			value.form_view, 
			prefix:			"form__",
			ID: 			value.form_id,
			mod:			value.form_mod, 
			color:			value.form_color, 
		} %}
	{% endif %}
	{% if value.type == "card_desc" %}
		{% include "../card/card.twig" with {
			view: value.view,
			prefix:"card__", 
			mod:  value.mod, 
			list: value.list
		} %}
	{% endif %} #}
</div>
{% endfor %}
{% endif %}